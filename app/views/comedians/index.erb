<html>
<head>
  <title>Comedians</title>
</head>
<body>
  <h1>Comedic Heroes & Heroines</h1>
  <% @comedians.each do |comedian| %>
    <ul>
      <li><%= comedian.name %></li>
      <li><%= comedian.age %></li>
    </ul>
  <% end %>
  <% @specials.each do |special| %>
    <ul>
      <li><%= special.name %></li>
    </ul>
  <% end %>
</body>
</html>
